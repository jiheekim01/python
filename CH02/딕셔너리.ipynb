# set : 집합 데이처 타입 
# set (literable) : literable => list, tuple, str
s1 = set([1,2,3])
print(s1)
print(s1[0])
# 에러다 : index 없다, 인덱스로 접근이 안된다 
# 값으로 접근 해야 한다

#중복 저거 하는 형태로 사용
s2 = set('Dello')
print(s2)

# 힙합 연산자 
s1 = set([1,2,3,4,5,6])
s2 = set([4,5,6,7,8,9])
print(s1, s2)

# 교빚합 
print(s1 & s2)
print(s1.intersection(s2))
print(s2.intersection(s2))

#합집합
print(s1 | s2)
print(s1.union(s2))
print(s2.union(s2))

# 차집합 
print(s1 - s2)
print(s1.difference(s2))
print(s2 - s1)
print(type(s2 - s1))
print(s2.difference(s1))

